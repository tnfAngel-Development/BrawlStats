{
  "name": "brawl-stats",
  "version": "1.0.0",
  "description": "BrawlStats Bot",
  "main": "src/index.ts",
  "scripts": {
    "start": "NODE_ENV=production ts-node src/index.ts",
    "start:dev": "NODE_ENV=development yarn nodemon src/index.ts",
    "lint": "yarn eslint .",
    "fix": "yarn eslint --fix .",
    "pretty": "yarn prettier --write .",
    "postinstall": "yarn run husky install",
    "commit": "lint-staged && yarn git-cz",
    "check": "yarn run ncu -p yarn --color",
    "update": "yarn run ncu -i -p yarn --color"
  },
  "keywords": [
    "BrawlStats Bot"
  ],
  "author": "tnfAngel",
  "license": "ISC",
  "dependencies": {
    "brawlstats": "^2.0.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "table": "^6.8.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.23",
    "@types/prettier": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
    "npm-check-updates": "^12.5.4",
    "prettier": "2.6.1",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "prettier --config .prettierrc --write",
      "eslint --fix \"src/**/*.{ts,tsx}\"",
      "eslint \"src/**/*.{ts,tsx}\""
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "packageManager": "yarn@3.1.0"
}
